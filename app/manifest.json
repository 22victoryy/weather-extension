{
    "name": "appname",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "appdescription",
    "icons": {
        "128": "images/icon-128.png"
    },
    "default_locale": "en",
    "background": {
        "scripts": [
            "weather/background.js"
        ]
    },
    "permissions": ["geolocation"],
    "browser_action": {
        "default_icon": {
            "38": "images/icon-38.png"
        },
        "default_title": "weather-display",
        "default_popup": "popup.html"
    }
}
